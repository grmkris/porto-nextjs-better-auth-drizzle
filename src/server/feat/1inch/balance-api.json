      "get": {
        "operationId": "ChainV1_2Controller_getAggregatedBalancesAndAllowancesV1_2_v1.2",
        "parameters": [
          {
            "name": "spender",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "wallets",
            "required": true,
            "in": "query",
            "explode": false,
            "description": "List of wallet addresses",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "filterEmpty",
            "required": false,
            "in": "query",
            "description": "Filter out empty balances and allowances",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Aggregated balances and allowances by tokens",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AggregatedBalancesAndAllowancesResponse"
                  }
                }
              }
            }
          }
        },
        "summary": "Get balances and allowances by spender for list of wallets addresses",
        "tags": ["Balances Endpoints"]
      }
    },
